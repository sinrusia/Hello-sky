/**********************************************************
/* Map Component Info
/**********************************************************/
.tobitItem{
	lineColor	: #FFFFFF;
	color		: #DEDEDE;
}

.componentInfo{
	color : #FFFFFF;
	lineColor : #FFFFFF;
	borderColor : #0000CC;
	borderStyle : "solid";
	borderThickness : 2;
	backgroundColor : #0066FF;
	backgroundAlpha : 1;
	fontFamily      : dotum;
	
}



