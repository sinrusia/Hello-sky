package com.wemb.tobit.vo
{
	
	[RemoteClass(alias="com.wemb.tobit.vo.TobitItem")]
	[Bindable]
	/**
	 * @author jaehag
	 */
	public class TobitItem extends ComponentInfo
	{
		public function TobitItem()
		{
			super();
		}
		/**
		 * 맵속성값
		 */
		private var _propValue:String;
		
		public function get propValue():String
		{
			return _propValue;
		}

		public function set propValue(value:String):void
		{
			_propValue = value;
		}
		/**
		 * 맵 속성명
		 */
		private var _propName:String;
		
		public function get propName():String
		{
			return _propName;
		}

		public function set propName(value:String):void
		{
			_propName = value;
		}
		
		private var _hostName:String = "";
		
		/** 장비 호스트 명 */
		public function get hostName():String
		{
			return _hostName;
		}
		
		/**
		 * @private
		 */
		public function set hostName(value:String):void
		{
			_hostName = value;
		}
		
		private var _ip:String = "";
		
		/** 장비 IP Address */
		public function get ip():String
		{
			return _ip;
		}
		
		/**
		 * @private
		 */
		public function set ip(value:String):void
		{
			_ip = value;
		}
		
	}
}